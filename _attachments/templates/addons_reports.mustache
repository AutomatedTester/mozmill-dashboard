    <fieldset id="filter">
      <legend>Filter</legend>
      <div id="branch-selection">
        <span>Branch:</span>
        <span>All</span>
        <span>7.0</span>
        <span>6.0</span>
        <span>5.0</span>
        <span>4.0</span>
        <span>3.6</span>
        <span>3.5</span>
      </div>

      <div id="os-selection">
        <span>OS: </span>
        <span>All</span>
        <span>Linux</span>
        <span>Mac</span>
        <span>Windows NT</span>
      </div>

      <div id="date">
        <span>From: </span>
        <input id="start-date" class="datepicker" type="text" value="" size="11" />
        <span>To: </span>
        <input id="end-date" class="datepicker" type="text" value="" size="11" />
      </div>
    </fieldset>

    <h2>Reports for Add-ons Tests</h2>

    <table id="results">
      <thead>
        <tr>
          <th>Date</th>
          <th>Version</th>
          <th>Build</th>
          <th>Locale</th>
          <th>Platform</th>
          <th>Version</th>
          <th>CPU</th>
          <th>Add-on</th>
          <th># Pass</th>
          <th># Skip</th>
          <th># Fail</th>
        </tr>
      </thead>
      <tbody>
      {{#reports}}
        <tr>
          <td><a href="{{report_link}}">{{time}}</a></td>
          <td>{{application_version}}</td>
          <td>{{build_id}}</td>
          <td>{{locale}}</td>
          <td>{{system_name}}</td>
          <td>{{system_version}}</td>
          <td>{{processor}}</td>
          <td>{{addon_name}} {{addon_version}}</td>
          <td class="results">{{tests_passed}}</td>
          <td class="results">{{tests_skipped}}</td>
          <td class="results">{{tests_failed}}</td>
        </tr>
      {{/reports}}
      </tbody>
    </table>
