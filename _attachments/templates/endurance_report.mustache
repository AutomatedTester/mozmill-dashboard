    <h2>Information</h2>
    <table id="information" class="clean">
      <tr>
       <th>Result ID:</th>
       <td><a href="{{path}}">{{id}}</a></td>
      </tr><tr>
       <th>Application:</th>
       <td>{{app_name}} {{app_version}} ({{platform_version}}, {{app_locale}}, {{platform_buildId}})</td>
      </tr><tr>
        <th>Sourcestamp:</th>
        <td><a href="{{app_sourcestamp}}">{{app_sourcestamp}}</a></td>
      </tr><tr>
        <th>Add-ons:</th>
        <td>
          <ul class="addons">
            <li>Extensions:
            <ul>
              {{#extensions}}<li>{{name}} {{version}}
                ({{#isActive}}<span class="passed">enabled</span>{{/isActive}}{{^isActive}}<span class="failed">disabled</span>{{/isActive}},
                {{#isCompatible}}<span class="passed">compatible</span>{{/isCompatible}}{{^isCompatible}}<span class="failed">incompatible</span>{{/isCompatible}})</li>
              {{/extensions}}
              {{^extensions}}<li>None</li>{{/extensions}}
            </ul></li>
            <li>Themes:
            <ul>
              {{#themes}}<li>{{name}} {{version}}
                ({{#isActive}}<span class="passed">enabled</span>{{/isActive}}{{^isActive}}<span class="failed">disabled</span>{{/isActive}},
                {{#isCompatible}}<span class="passed">compatible</span>{{/isCompatible}}{{^isCompatible}}<span class="failed">incompatible</span>{{/isCompatible}})</li>
              {{/themes}}
              {{^themes}}<li>None</li>{{/themes}}
            </ul></li>
            <li>Plugins:
            <ul>
              {{#plugins}}<li>{{name}} {{version}}
                ({{#isActive}}<span class="passed">enabled</span>{{/isActive}}{{^isActive}}<span class="failed">disabled</span>{{/isActive}},
                {{#isCompatible}}<span class="passed">compatible</span>{{/isCompatible}}{{^isCompatible}}<span class="failed">incompatible</span>{{/isCompatible}})</li>
              {{/plugins}}
              {{^plugins}}<li>None</li>{{/plugins}}
            </ul></li>
          </ul>
        </td>
      </tr><tr>
        <th>Platform:</th>
        <td>{{system}} {{system_version}} {{service_pack}} ({{cpu}})</td>
      </tr><tr>
        <th>Duration:</th>
        <td>{{time_start}} - {{time_end}}</td>
      </tr><tr>
        <th>Results:</th>
        <td>Pass: <strong>{{passed}}</strong> / Fail: <strong>{{failed}}</strong> / Skip: <strong>{{skipped}}</td>
      </tr>
    </table>

    <h2>Endurance</h2>
    <table class="clean">
      <tbody>
        <tr>
          <th>Arguments:</th>
          <td>Delay (s): <strong>{{delay}}</strong> / Iterations: <strong>{{iterations}}</strong> / Restart: <strong>{{#restart}}Yes{{/restart}}{{^restart}}No{{/restart}}</strong></td>
        </tr><tr>
          <th>Number of tests:</th>
          <td>{{testCount}}</td>
        </tr><tr>
          <th>Number of checkpoints:</th>
          <td>{{checkpointCount}} ({{checkpointsPerTest}} per test)
          </td>
        </tr><tr>
          <th>Allocated memory (MB):</th>
          <td>Minimum: <strong>{{minAllocatedMemory}}</strong> / Maximum: <strong>{{maxAllocatedMemory}}</strong> / Average: <strong>{{averageAllocatedMemory}}</strong></td>
        </tr><tr>
          <th>Mapped memory (MB):</th>
          <td>Minimum: <strong>{{minMappedMemory}}</strong> / Maximum: <strong>{{maxMappedMemory}}</strong> / Average: <strong>{{averageMappedMemory}}</strong></td>
        </tr>
      </tbody>
    </table>

    <script>
        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'checkpoints-memory-usage-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            plotOptions: {
                line: {
                    marker: {
                        enabled: false
                    }
                },
                series: {
                    pointStart: 1
                    }
            },
            series: [{
                name: 'Allocated',
                data: [{{#checkpoints}}{
                    name: "<strong>File:</strong> {{testFile}}<br/><strong>Method:</strong> {{testMethod}}<br/><strong>Checkpoint:</strong> {{label}}",
                    y: {{allocatedMemory}}
                },{{/checkpoints}}]
            }, {
                name: 'Mapped',
                data: [{{#checkpoints}}{
                    name: "<strong>File:</strong> {{testFile}}<br/><strong>Method:</strong> {{testMethod}}<br/><strong>Checkpoint:</strong> {{label}}",
                    y: {{mappedMemory}}
                },{{/checkpoints}}]
            }],
            title: {
                text: 'Memory Usage (' + {{checkpointCount}} + ' checkpoints)'
            },
            tooltip: {
                formatter: function() {
                    return this.point.name + '<br/>' +
                        '<strong>' + this.series.name + ' memory: </strong>' + this.y + ' MB';
                }
            },
            xAxis: {
                allowDecimals: false,
                gridLineWidth: 1,
                title: {
                    text: 'Checkpoints'
                },
            },
            yAxis: {
                title: {
                    text: 'Memory Usage (MB)'
                },
                min: 0
            },
        });

        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'tests-memory-usage-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            plotOptions: {
                line: {
                    marker: {
                        enabled: false
                    }
                },
                series: {
                    pointStart: 1
                    }
            },
            series: [{
                name: 'Allocated',
                data: [{{#tests}}{
                    name: "<strong>File:</strong> {{testFile}}<br/><strong>Method:</strong> {{testMethod}}",
                    y: {{averageAllocatedMemory}}
                },{{/tests}}]
            }, {
                name: 'Mapped',
                data: [{{#tests}}{
                    name: "<strong>File:</strong> {{testFile}}<br/><strong>Method:</strong> {{testMethod}}",
                    y: {{averageMappedMemory}}
                },{{/tests}}]
            }],
            title: {
                text: 'Memory Usage (' + {{testCount}} + ' tests)'
            },
            tooltip: {
                formatter: function() {
                    return this.point.name + '<br/>' +
                        '<strong>' + this.series.name + ' memory: </strong>' + this.y + ' MB';
                }
            },
            xAxis: {
                allowDecimals: false,
                gridLineWidth: 1,
                title: {
                    text: 'Tests'
                },
            },
            yAxis: {
                title: {
                    text: 'Memory Usage (MB)'
                },
                min: 0
            }
        });
    </script>

    <div id="checkpoints-memory-usage-chart" class="chart" style="float:left"></div>
    <div id="tests-memory-usage-chart" class="chart"></div>
    
    <h2>Results</h2>

    <table id="results">
      <thead>
        <tr>
          <th rowspan="2">Filename</th>
          <th rowspan="2">Test</th>
          <th colspan="3">Allocated Memory (MB)</th>
          <th colspan="3">Mapped Memory (MB)</th>
        </tr><tr>
          <th>min</th>
          <th>max</th>
          <th>average</th>
          <th>min</th>
          <th>max</th>
          <th>average</th>
        </tr>
      </thead>
      <tbody>
        {{#tests}}
        <tr>
          <td>{{testFile}}</td>
          <td>{{testMethod}}</td>
          <td class="results">{{minAllocatedMemory}}</td>
          <td class="results">{{maxAllocatedMemory}}</td>
          <td class="results">{{averageAllocatedMemory}}</td>
          <td class="results">{{minMappedMemory}}</td>
          <td class="results">{{maxMappedMemory}}</td>
          <td class="results">{{averageMappedMemory}}</td>
        </tr>
        {{/tests}}
      </tbody>
    </table>
  </body>

