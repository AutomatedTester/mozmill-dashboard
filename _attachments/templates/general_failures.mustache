    <fieldset id="filter">
      <legend>Filter</legend>
      <div id="branch-selection">
        <span>Platform:</span>
        <span>All</span>
        <span>4.0</span>
        <span>3.6</span>
        <span>3.5</span>
      </div>

      <div id="os-selection">
        <span>OS: </span>
        <span>All</span>
        <span>Linux</span>
        <span>Mac</span>
        <span>Windows NT</span>
      </div>
    </fieldset>

    <table id="results">
      <thead>
        <tr>
          <th>Test Module</th>
          <th>Version</th>
          <th>Platform</th>
          <th># Fail</th>
          <!--<th>Bugs</th>-->
        </tr>
      </thead>
      <tbody>
      {{#reports}}
        <tr>
          <td>{{test_module}}</td>
          <td>{{application_version}}</td>
          <td>{{system_name}}</td>
          <td><a href="{{failure_link}}">{{failure_count}}</a></td>
          <!--<td>{{bugs}}</td>-->
        </tr>
      {{/reports}}
      </tbody>
    </table>

    <div id="pagination"><input class="pagination" type="text" value="20"></input><span class="pagination"> more</span></div>  
